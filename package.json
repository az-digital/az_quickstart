    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^10.1.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-yml": "^1.18.0",
    "glob": "11.0.3",
