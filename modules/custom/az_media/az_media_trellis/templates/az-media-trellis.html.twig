{#
/**
 * @file
 * Template implementation for the az_media_trellis theme hook.
 *
 * Available variables:
 * - url: (string) A formatted Trellis source URL
 * - view_mode: (string) The current view mode (e.g., 'default', 'az_medium', 'az_small')
 */
#}
{{ attach_library('az_media_trellis/az-media-trellis') }}
<div id="{{ target_id }}" class="az-media-trellis az-media-trellis--{{ view_mode|clean_class }}"></div>
<script src="{{ url }}" data-qp-target-id="{{ target_id }}" defer></script>
