{
    "name": "az-digital/az_quickstart",
    "description": "Arizona Quickstart",
    "type": "drupal-custom-profile",
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "Arizona Digital",
            "homepage": "https://digital.arizona.edu",
            "role": "Team"
        }
    ],
    "support": {
        "docs": "https://quickstart.arizona.edu/help"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "php": ">=7.0",
        "composer/installers": "1.7.0",
        "cweagans/composer-patches": "1.6.7",
        "drupal/background_image_formatter": "1.7",
        "drupal/bootstrap_barrio": "4.30",
        "drupal/cas": "1.7",
        "drupal/core-recommended": "8.9.2",
        "drush/drush": "9.7.1",
        "drupal/ds": "3.x-dev#170540060a644102ab3524cd1d31de408d492186",
        "drupal/easy_breadcrumb": "1.13",
        "drupal/externalauth": "1.3",
        "drupal/field_group": "3.1",
        "drupal/layout_builder_restrictions": "2.7",
        "drupal/layout_builder_styles": "1.0-beta2",
        "drupal/media_library_theme_reset": "1.0",
        "drupal/migrate_plus": "5.1",
        "drupal/migrate_tools": "5.0",
        "drupal/pathauto": "1.8",
        "drupal/token": "1.7",
        "drupal/paragraphs": "1.12",
        "joshtronic/php-loremipsum": "1.0.6"
    },
    "require-dev": {
        "az-digital/az-quickstart-dev": "dev-master"
    },
    "extra": {
        "patches": {
            "drupal/core": {
                "ajax css load order issue": "https://www.drupal.org/files/issues/2020-03-31/1461322-23.patch",
                "Layout builder revisions issue": "https://www.drupal.org/files/issues/2019-06-17/3033516-17.patch",
                "Quickedit attributes issue": "https://www.drupal.org/files/issues/2020-06-18/3072231-26-core-8-9-x-.patch"
            },
            "drupal/ds": {
                "Remove drush plugin": "https://git.drupalcode.org/project/ds/commit/c8db6ed6f5f2a543f9fe50d440a5dd94a1f13fae.diff"
            },
            "drupal/easy_breadcrumb": {
                "Easy_breadcrumb schema issue": "https://www.drupal.org/files/issues/2020-07-27/missing_schema_keys-3161765-4.patch"
            }
        }
    }
}
