#!/bin/bash

## Description: Provide phpunit tooling to run unit tests.
## Usage: phpunit [flags] [args]
## Example: "ddev phpunit --version"

vendor/bin/phpunit --configuration /var/www/html/web/profiles/custom/az_quickstart/phpunit.xml.dist --bootstrap /var/www/html/web/core/tests/bootstrap.php /var/www/html/web/profiles/custom/az_quickstart $@
